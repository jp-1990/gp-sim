{
  "name": "gp-sim-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "prepare": "husky install",
    "test": "jest --watch",
    "extract-intl": "formatjs extract 'src/**/*.ts*' '--ignore=**/*.d.ts' --out-file lang/en.json --id-interpolation-pattern [sha512:contenthash:base64:6] --format simple"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.11",
    "@chakra-ui/react": "^2.3.5",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@fontsource/lato": "^4.5.5",
    "@reduxjs/toolkit": "^1.8.5",
    "axios": "^0.27.2",
    "file-saver": "^2.0.5",
    "firebase": "^9.13.0",
    "firebase-admin": "^11.1.0",
    "formidable": "^2.0.1",
    "framer-motion": "^7.5.3",
    "jszip": "^3.9.1",
    "lodash": "^4.17.21",
    "next": "^12.3.1",
    "next-redux-wrapper": "^7.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^5.24.8",
    "react-redux": "^8.0.4",
    "sharp": "^0.31.1"
  },
  "devDependencies": {
    "@formatjs/cli": "^4.8.3",
    "@testing-library/dom": "^8.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/file-saver": "^2.0.5",
    "@types/formidable": "^2.0.5",
    "@types/node": "17.0.23",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/sharp": "^0.31.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "eslint": "^8.2.0",
    "eslint-config-next": "12.1.1",
    "husky": "^7.0.4",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "lint-staged": "^12.3.7",
    "msw": "0.36.7",
    "prettier": "^2.6.1",
    "typescript": "4.6.3"
  },
  "lint-staged": {
    "!(jest*)*.{js,ts,tsx}": [
      "eslint --cache --fix",
      "jest --bail --findRelatedTests --passWithNoTests"
    ],
    "!(jest*)*.{js,ts,tsx,css,md}": "prettier --write"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
