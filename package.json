{
  "name": "gp-sim-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "test": "jest --watch",
    "extract-intl": "formatjs extract 'src/**/*.ts*' '--ignore=**/*.d.ts' --out-file lang/en.json --id-interpolation-pattern [sha512:contenthash:base64:6] --format simple"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^1.7.0",
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.7",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@fontsource/lato": "^4.5.5",
    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^0.27.2",
    "file-saver": "^2.0.5",
    "framer-motion": "^6",
    "jszip": "^3.9.1",
    "lodash": "^4.17.21",
    "next": "12.1.1",
    "next-redux-wrapper": "^7.0.5",
    "react": "^17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "^5.24.8",
    "react-redux": "^7.2.8"
  },
  "devDependencies": {
    "@formatjs/cli": "^4.8.3",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^14.1.0",
    "@types/file-saver": "^2.0.5",
    "@types/node": "17.0.23",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "eslint": "^8.2.0",
    "eslint-config-next": "12.1.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "msw": "^0.39.2",
    "prettier": "^2.6.1",
    "typescript": "4.6.3"
  },
  "lint-staged": {
    "!(jest*)*.{js,ts,tsx}": [
      "eslint --cache --fix",
      "jest --bail --findRelatedTests"
    ],
    "!(jest*)*.{js,ts,tsx,css,md}": "prettier --write"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
